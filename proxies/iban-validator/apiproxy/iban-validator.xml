<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy name="iban-validator">
    <Basepaths>/html2pdf</Basepaths>
    <Description>The iban-validator service</Description>
    <DisplayName>iban-vaiban-validator</DisplayName>
    <Policies>
        <Policy>add-cors</Policy>
        <Policy>sharedAuthWithOkta</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>default</ProxyEndpoint>
    </ProxyEndpoints>
    <Resources/>
    <Spec></Spec>
    <TargetServers/>
    <TargetEndpoints>
        <TargetEndpoint>k8s-develop</TargetEndpoint>
        <TargetEndpoint>k8s-prod</TargetEndpoint>
        <TargetEndpoint>k8s-stage</TargetEndpoint>
    </TargetEndpoints>
</APIProxy>
