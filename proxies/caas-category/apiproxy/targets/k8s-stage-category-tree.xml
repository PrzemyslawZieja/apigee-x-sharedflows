<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="k8s-stage-category-tree">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>Rewrite-Path-Suffix</Name>
                <Condition>(((proxy.pathsuffix MatchesPath "/mpreistest/categories/categoryTree") or (proxy.pathsuffix MatchesPath "/saastest2/categories/categoryTree") or (proxy.pathsuffix MatchesPath "/oddity/categories/categoryTree")) and (request.verb = "GET"))</Condition>
            </Step>
            <Step>
                <Name>Rewrite-Revursive-param</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>https://category-tree.k8s-stage.emporix.io</URL>
    </HTTPTargetConnection>
</TargetEndpoint>