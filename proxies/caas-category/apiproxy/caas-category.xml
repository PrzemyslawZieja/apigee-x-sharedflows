<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy name="caas-category">
    <Basepaths>/caas-category</Basepaths>
    <Description></Description>
    <DisplayName>caas-category</DisplayName>
    <Policies>
        <Policy>Rewrite-Path-Suffix</Policy>
        <Policy>Rewrite-Revursive-param</Policy>
        <Policy>sharedAuthWithOkta</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>default</ProxyEndpoint>
        <ProxyEndpoint>emporix</ProxyEndpoint>
    </ProxyEndpoints>
    <Resources>
        <Resource>jsc://Rewrite-Revursive-param.js</Resource>
        <Resource>jsc://rewritePathSuffix.js</Resource>
    </Resources>
    <Spec></Spec>
    <TargetServers/>
    <TargetEndpoints>
        <TargetEndpoint>k8s-develop-category-tree</TargetEndpoint>
        <TargetEndpoint>k8s-develop</TargetEndpoint>
        <TargetEndpoint>k8s-prod-category-tree</TargetEndpoint>
        <TargetEndpoint>k8s-prod</TargetEndpoint>
        <TargetEndpoint>k8s-stage-category-tree</TargetEndpoint>
        <TargetEndpoint>k8s-stage</TargetEndpoint>
    </TargetEndpoints>
</APIProxy>
