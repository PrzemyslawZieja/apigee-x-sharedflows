<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy name="price">
    <Basepaths>/price</Basepaths>
    <Description>The price calculation service v3.</Description>
    <DisplayName>price</DisplayName>
    <Policies>
        <Policy>saasag.prod_price_calculate</Policy>
        <Policy>sharedAuthWithOkta</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>default</ProxyEndpoint>
        <ProxyEndpoint>emporix</ProxyEndpoint>
    </ProxyEndpoints>
    <Resources/>
    <Spec></Spec>
    <TargetServers/>
    <TargetEndpoints>
        <TargetEndpoint>k8s-deprecated-develop</TargetEndpoint>
        <TargetEndpoint>k8s-deprecated-prod</TargetEndpoint>
        <TargetEndpoint>k8s-deprecated-stage</TargetEndpoint>
        <TargetEndpoint>k8s-develop</TargetEndpoint>
        <TargetEndpoint>k8s-prod</TargetEndpoint>
        <TargetEndpoint>k8s-stage</TargetEndpoint>
        <TargetEndpoint>k8s-v2-develop</TargetEndpoint>
        <TargetEndpoint>k8s-v2-prod</TargetEndpoint>
        <TargetEndpoint>k8s-v2-stage</TargetEndpoint>
    </TargetEndpoints>
</APIProxy>
