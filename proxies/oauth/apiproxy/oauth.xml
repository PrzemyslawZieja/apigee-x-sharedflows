<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy name="oauth">
    <Basepaths>/oauth/client_credential</Basepaths>
    <Description>An API proxy that enforces OAuth 2.0 Client Credential Grant flow</Description>
    <Policies>
        <Policy>Add-Tenant-Scope</Policy>
        <Policy>Add-default-tenant-scope</Policy>
        <Policy>AddHybrisSessionIdFromParam</Policy>
        <Policy>AddSessionIdFromParam</Policy>
        <Policy>Assign-Fake-Password</Policy>
        <Policy>Assign-Fake-Username</Policy>
        <Policy>Assign-Grant-Type-Paasword</Policy>
        <Policy>Build-Anonymous-Token-Response</Policy>
        <Policy>Build-Refresh-Token-Response</Policy>
        <Policy>Decode-Basic-Auth</Policy>
        <Policy>DecodeBaseAuthHeaders</Policy>
        <Policy>Generate-Anonymous-Access-Token</Policy>
        <Policy>Generate1HOURAccessTokenClient</Policy>
        <Policy>Generate1MINUTEAccessTokenClient</Policy>
        <Policy>GenerateAccessTokenClient</Policy>
        <Policy>GenerateSessionId</Policy>
        <Policy>GetATCustomAttributes</Policy>
        <Policy>Match-Client-With-App</Policy>
        <Policy>Refresh-Anonymous-Access-Token</Policy>
        <Policy>RefreshAccessToken</Policy>
        <Policy>RevokeToken</Policy>
        <Policy>RewriteRefreshToken</Policy>
        <Policy>RewriteToken</Policy>
        <Policy>RewriteTokenWithIAM</Policy>
        <Policy>Spike-Arrest</Policy>
        <Policy>Verify-API-Key</Policy>
        <Policy>Wrong-Grant-Type</Policy>
        <Policy>debug</Policy>
        <Policy>testregenerate</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>customerlogin</ProxyEndpoint>
        <ProxyEndpoint>default</ProxyEndpoint>
        <ProxyEndpoint>oauth2</ProxyEndpoint>
        <ProxyEndpoint>oldHybrisRoute</ProxyEndpoint>
        <ProxyEndpoint>oldRoute</ProxyEndpoint>
    </ProxyEndpoints>
    <Resources>
        <Resource>jsc://add-default-tenant-scope.js</Resource>
        <Resource>jsc://debug.js</Resource>
    </Resources>
    <Spec></Spec>
    <TargetServers/>
    <TargetEndpoints>
        <TargetEndpoint>default</TargetEndpoint>
    </TargetEndpoints>
</APIProxy>
