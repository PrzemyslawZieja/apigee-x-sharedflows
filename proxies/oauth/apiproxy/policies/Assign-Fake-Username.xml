<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Assign-Fake-Username">
    <DisplayName>Assing Fake Username</DisplayName>
    <AssignVariable>
        <Name>request.formparam.username</Name>
        <Template>FakeUser</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>