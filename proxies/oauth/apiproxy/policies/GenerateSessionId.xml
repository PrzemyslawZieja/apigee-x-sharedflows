<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="GenerateSessionId">
    <DisplayName>Generate Session Id</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>request.queryparam.session-id</Name>
        <Template>{createUuid()}</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>