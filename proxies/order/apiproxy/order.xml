<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy name="order">
    <Basepaths>/order</Basepaths>
    <ConfigurationVersion majorVersion="4" minorVersion="0"/>
    <Description>Saas Order Service to manage orders from the frontend</Description>
    <DisplayName>order</DisplayName>
    <Policies>
        <Policy>ClientIpAddress</Policy>
        <Policy>sharedAuthWithOkta</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>default</ProxyEndpoint>
        <ProxyEndpoint>emporix</ProxyEndpoint>
    </ProxyEndpoints>
    <Resources/>
    <Spec></Spec>
    <TargetServers/>
    <TargetEndpoints>
        <TargetEndpoint>k8s-develop</TargetEndpoint>
        <TargetEndpoint>k8s-stage</TargetEndpoint>
        <TargetEndpoint>k8s-prod</TargetEndpoint>
    </TargetEndpoints>
</APIProxy>
