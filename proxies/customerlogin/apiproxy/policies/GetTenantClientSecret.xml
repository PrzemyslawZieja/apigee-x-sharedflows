<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="GetTenantClientSecret" mapIdentifier="tenants">
    <DisplayName>GetTenants</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>600</ExpiryTimeInSecs>
    <Get assignTo="variable.client_secret">
        <Key>
            <Parameter ref="tenant"/>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>