<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy name="caas-customer">
    <Basepaths>/caas-customer</Basepaths>
    <Description></Description>
    <DisplayName>caas-customer</DisplayName>
    <Policies>
        <Policy>AesCallout</Policy>
        <Policy>AesKVM</Policy>
        <Policy>sharedBasicAuth</Policy>
        <Policy>ExecuteAuth0Request</Policy>
        <Policy>ExecuteClientConfigurationRequest</Policy>
        <Policy>ExecuteTokenRequest</Policy>
        <Policy>ExtractAuth0Response</Policy>
        <Policy>ExtractCustomerResponse</Policy>
        <Policy>ExtractTokenResponse</Policy>
        <Policy>GenerateSaasToken</Policy>
        <Policy>Key-Value-Map-Operations-1</Policy>
        <Policy>PrepareCustomerRequest</Policy>
        <Policy>RewriteToken</Policy>
        <Policy>VerifyAuth0Token</Policy>
        <Policy>extractConfigurationResponse</Policy>
        <Policy>extractSocialLoginRequestVariables</Policy>
        <Policy>parseAuth0Token</Policy>
        <Policy>parseClientSecret</Policy>
        <Policy>parseConfigurationEnvironment</Policy>
        <Policy>prepareAuth0Request</Policy>
        <Policy>prepareConfigurationRequest</Policy>
        <Policy>sharedAuthWithOkta</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>default</ProxyEndpoint>
        <ProxyEndpoint>emporix</ProxyEndpoint>
    </ProxyEndpoints>
    <Resources>
        <Resource>java://edge-callout-aes-crypto-20200616.jar</Resource>
        <Resource>jsc://parseAuth0Token.js</Resource>
        <Resource>jsc://parseConfigurationDomain.js</Resource>
        <Resource>jsc://parseConfigurationEnvironment.js</Resource>
    </Resources>
    <Spec></Spec>
    <TargetServers/>
    <TargetEndpoints>
        <TargetEndpoint>k8s-develop</TargetEndpoint>
        <TargetEndpoint>k8s-prod</TargetEndpoint>
        <TargetEndpoint>k8s-stage</TargetEndpoint>
    </TargetEndpoints>
</APIProxy>
