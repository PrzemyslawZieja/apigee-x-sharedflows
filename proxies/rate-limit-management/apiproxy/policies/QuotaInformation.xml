<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage name="QuotaInformation">
  <Set>
    <StatusCode>200</StatusCode>
    <Payload contentType="application/json">
      {
        "tenant":"{tenant}",
        "quota-available-count":"{ratelimit.QuotaPolicy.available.count}",
        "quota-reset-time":"{ratelimit.QuotaPolicy.expiry.time}",
        "quota-interval":"{AccessEntity.GetQuotaFromApiProduct.ApiProduct.QuotaInterval}",
        "quota-time-unit":"{AccessEntity.GetQuotaFromApiProduct.ApiProduct.QuotaTimeUnit}",
        "quota-allowed-count":"{ratelimit.QuotaPolicy.allowed.count}"
      }
    </Payload>
  </Set>
</AssignMessage>
