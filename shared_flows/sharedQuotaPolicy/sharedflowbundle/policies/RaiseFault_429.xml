<RaiseFault name="RaiseFault_429">
 <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
 <FaultResponse>
   <Set>
     <StatusCode>429</StatusCode>
     <ReasonPhrase>Requests quota reached for tenant.</ReasonPhrase>
     <Payload contentType="application/json">
        {"fault":"Requests quota reached for tenant.", "tenant":"{tenant}"}
     </Payload>
   </Set>
 </FaultResponse>
</RaiseFault>