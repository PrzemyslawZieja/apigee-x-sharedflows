<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" mapIdentifier="environmentVariables" name="getCredentials">
    <DisplayName>getCredentials</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>86400</ExpiryTimeInSecs>
    <Get assignTo="credentials.username">
        <Key>
            <Parameter>basicAuth_username</Parameter>
        </Key>
    </Get>
    <Get assignTo="credentials.password">
        <Key>
            <Parameter>basicAuth_password</Parameter>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>
