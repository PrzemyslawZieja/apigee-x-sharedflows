<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="BasicAuthKVM" mapIdentifier="environmentVariables">
    <DisplayName>BasicAuthKVM</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>1</ExpiryTimeInSecs>
    <Get assignTo="private.credentials.username">
        <Key>
            <Parameter>basicAuth_username</Parameter>
        </Key>
    </Get>
    <Get assignTo="private.credentials.password">
        <Key>
            <Parameter>basicAuth_password</Parameter>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>